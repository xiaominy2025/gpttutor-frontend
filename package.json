{
  "name": "gpttutor-frontend",
  "private": true,
  "version": "1.6.6.6",
  "homepage": "/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:log": "mkdir -p test-logs && npx playwright test --reporter=line,html > test-logs/test-output.log 2>&1",
    "deploy:aws": "node deploy-aws.js",
    "deploy:s3": "node deploy-s3.js",
    "deploy:build": "npm run build && echo 'Build completed. Run deploy:s3 to deploy to S3.'",
    "deploy:test": "npm run build && npm run preview",
    "env:check": "node -e \"console.log('Environment Variables:'); console.log('VITE_API_BASE_URL:', process.env.VITE_API_BASE_URL || 'Not set'); console.log('VITE_BACKEND_URL:', process.env.VITE_BACKEND_URL || 'Not set'); console.log('VITE_DEPLOYMENT:', process.env.VITE_DEPLOYMENT || 'Not set');\"",
    "test:backend": "node test-backend-response.js",
    "deploy:engentlabs": "powershell -ExecutionPolicy Bypass -File scripts/deploy-engentlabs-final.ps1",
    "deploy:engentlabs:dry": "powershell -ExecutionPolicy Bypass -File scripts/deploy-engentlabs-final.ps1 -DryRun",
    "deploy:engentlabs:skip-build": "powershell -ExecutionPolicy Bypass -File scripts/deploy-engentlabs-final.ps1 -SkipBuild",
    "deploy:engentlabs:skip-invalidation": "powershell -ExecutionPolicy Bypass -File scripts/deploy-engentlabs-final.ps1 -SkipInvalidation",
    "deploy:engentlabs:linux": "bash scripts/deploy-engentlabs.sh",
    "deploy:engentlabs:linux:dry": "bash scripts/deploy-engentlabs.sh --dry-run",
    "deploy:engentlabs:linux:skip-build": "bash scripts/deploy-engentlabs.sh --skip-build",
    "deploy:engentlabs:linux:skip-invalidation": "bash scripts/deploy-engentlabs.sh --skip-invalidation"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  }
}
